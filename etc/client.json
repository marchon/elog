{
    "apps": [
        {
            "name": "app name",
            "file": "/tmp/php_errors.log",
            "interval_time": 5000,
            "position": 0,
            "rules": {
                "include": [ 
                    [["error", "i"], "LOG_ERROR"],
                    ["Notice", "LOG_WARN"]
                ],
                "exclude": ["Primary script unknown"]
            },
        },
        {
            "name": "nginx",
            "file": "/usr/local/nginx/logs/error.log",
            "interval_time": 5000,
            "position": 0,
            "rules": {
                "include": [
                    ["error", "LOG_ERROR"],
                    ["alert", "LOG_WARN"]
                ],
                "exclude": [""]
            }
        }
    ],
    "api": {
        "key": "mykey",
        "url": "http://localhost:3339/api"
    }
}
